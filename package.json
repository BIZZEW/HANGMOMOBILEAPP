        "axios": "^0.19.1",
        "moment": "^2.24.0",
        "patch-package": "^6.2.2",
        "rc-form": "^2.4.11",
        "react": "16.9.0",
        "react-dom": "^16.12.0",
        "react-native": "^0.61.5",
        "react-native-gesture-handler": "^1.5.3",
